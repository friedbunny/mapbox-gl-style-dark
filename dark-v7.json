{
  "version": 7,
  "name": "Dark",
  "constants": {
    "@name": "{name}",
    "@sans": "Source Sans Pro Regular, Arial Unicode MS Regular",
    "@sans_md": "Source Sans Pro Semibold, Arial Unicode MS Bold",
    "@sans_bd": "Source Sans Pro Bold, Arial Unicode MS Bold",
    "@sans_it": "Source Sans Pro Italic, Arial Unicode MS Regular",
    "@ride-line-opacity": 0.5,
    "@land": "#111",
    "@water": "#222",
    "@park": "#050505",
    "@building": "#191919",
    "@label": "#888",
    "@label-halo": "rgba(0,0,0,0.9)",
    "@road-major": "#282828",
    "@road-minor": "#252525",
    "@road-major-width": {
      "base": 1.4,
      "stops": [[6, 0.5], [20, 30]]
    },
    "@road-minor-width": {
      "base": 1.55,
      "stops": [[4, 0.25], [20, 30]]
    },
    "@major_rail_hatching_width": {
      "stops": [[12, 5], [20, 10]]
    },
    "@major_rail_hatching_dasharray": [0.05, 10],
    "@railroad": "#333",
    "@path": "#555",
    "@path_width": {
      "base": 1,
      "stops": [[15, 1.25], [20, 2]]
    },
    "@path_steps_width": {
      "base": 1.3,
      "stops": [[15, 1.5], [20, 10]]
    },
    "@path_dasharray_primary": [3, 1]
  },
  "sources": {
    "mapbox": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-streets-v6",
      "maxZoom": 15
    }
  },
  "sprite": "//friedbunny.github.io/mapbox-gl-style-dark/sprites/dark",
  "glyphs": "mapbox://fontstack/{fontstack}/{range}.pbf",
  "layers": [{
    "id": "background",
    "type": "background",
    "paint": {
      "background-color": "@land"
    }
  }, {
    "id": "landuse_park",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "landuse",
    "filter": ["==", "class", "park"],
    "paint": {
      "fill-color": "@park"
    }
  }, {
    "id": "waterway",
    "type": "line",
    "source": "mapbox",
    "source-layer": "waterway",
    "filter": ["all", ["==", "$type", "LineString"], ["in", "class", "river", "canal"]],
    "paint": {
      "line-color": "@water",
      "line-width": {
        "base": 1.4,
        "stops": [[8, 0.5], [20, 15]]
      }
    }
  }, {
    "id": "water",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "water",
    "paint": {
      "fill-color": "@water"
    }
  }, {
    "id": "building",
    "type": "fill",
    "source": "mapbox",
    "source-layer": "building",
    "paint": {
      "fill-color": "@building"
    }
  }, {
    "id": "tunnel_minor",
    "type": "line",
    "source": "mapbox",
    "source-layer": "tunnel",
    "filter": ["all", ["==", "$type", "LineString"], ["in", "class", "motorway_link", "street", "street_limited", "service", "driveway", "path"]],
    "paint": {
      "line-color": "@road-minor",
      "line-width": "@road-minor-width",
      "line-dasharray": [0.36, 0.18]
    }
  }, {
    "id": "tunnel_major",
    "type": "line",
    "source": "mapbox",
    "source-layer": "tunnel",
    "filter": ["all", ["==", "$type", "LineString"], ["in", "class", "motorway", "main"]],
    "paint": {
      "line-color": "@road-major",
      "line-width": "@road-major-width",
      "line-dasharray": [0.28, 0.14]
    }
  }, {
    "id": "road_minor",
    "type": "line",
    "source": "mapbox",
    "source-layer": "road",
    "filter": ["all", ["in", "class", "motorway_link", "street", "street_limited", "service", "driveway", "path"], ["==", "$type", "LineString"]],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "@road-minor",
      "line-width": "@road-minor-width"
    }
  }, {
    "id": "road_major",
    "type": "line",
    "source": "mapbox",
    "source-layer": "road",
    "filter": ["all", ["==", "$type", "LineString"], ["in", "class", "motorway", "main"]],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "@road-major",
      "line-width": "@road-major-width"
    }
  }, {
    "id": "bridge_minor_case",
    "type": "line",
    "source": "mapbox",
    "source-layer": "bridge",
    "filter": ["all", ["==", "$type", "LineString"], ["in", "class", "motorway_link", "street", "street_limited", "service", "driveway", "path"]],
    "paint": {
      "line-color": "@land",
      "line-width": {
        "base": 1.6,
        "stops": [[12, 0.5], [20, 10]]
      },
      "line-gap-width": "@road-minor-width"
    }
  }, {
    "id": "bridge_major_case",
    "type": "line",
    "source": "mapbox",
    "source-layer": "bridge",
    "filter": ["all", ["in", "class", "motorway", "main"], ["==", "$type", "LineString"]],
    "paint": {
      "line-color": "@land",
      "line-width": {
        "base": 1.4,
        "stops": [[12, 0.5], [20, 10]]
      },
      "line-gap-width": "@road-major-width"
    }
  }, {
    "id": "bridge_minor",
    "type": "line",
    "source": "mapbox",
    "source-layer": "bridge",
    "filter": ["all", ["==", "$type", "LineString"], ["in", "class", "motorway_link", "street", "street_limited", "service", "driveway", "path"]],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "@road-minor",
      "line-width": "@road-minor-width"
    }
  }, {
    "id": "bridge_major",
    "type": "line",
    "source": "mapbox",
    "source-layer": "bridge",
    "filter": ["all", ["in", "class", "motorway", "main"], ["==", "$type", "LineString"]],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "@road-major",
      "line-width": "@road-major-width"
    }
  }, {
    "id": "tunnel_major_rail",
    "type": "line",
    "source": "mapbox",
    "source-layer": "tunnel",
    "filter": ["==", "class", "major_rail"],
    "paint": {
      "line-color": "@railroad",
      "line-width": 0.8
    }
  }, {
    "id": "tunnel_major_rail_hatching",
    "ref": "tunnel_major_rail",
    "paint": {
      "line-color": "@road-major",
      "line-dasharray": "@major_rail_hatching_dasharray",
      "line-width": "@major_rail_hatching_width"
    }
  }, {
    "id": "road_major_rail",
    "type": "line",
    "source": "mapbox",
    "source-layer": "road",
    "filter": ["==", "class", "major_rail"],
    "paint": {
      "line-color": "@railroad",
      "line-width": 0.8
    }
  }, {
    "id": "road_major_rail_hatching",
    "ref": "road_major_rail",
    "paint": {
      "line-color": "@railroad",
      "line-dasharray": "@major_rail_hatching_dasharray",
      "line-width": "@major_rail_hatching_width"
    }
  }, {
    "id": "bridge_major_rail",
    "type": "line",
    "source": "mapbox",
    "source-layer": "bridge",
    "filter": ["==", "class", "major_rail"],
    "paint": {
      "line-color": "@railroad",
      "line-width": 0.8
    }
  }, {
    "id": "bridge_major_rail_hatching",
    "ref": "bridge_major_rail",
    "paint": {
      "line-color": "@railroad",
      "line-dasharray": "@major_rail_hatching_dasharray",
      "line-width": "@major_rail_hatching_width"
    }
  }, {
    "id": "road_path_awesome_modes",
    "type": "line",
    "source": "mapbox",
    "source-layer": "road",
    "filter": ["in", "type", "footway", "path", "other", "hiking", "trail", "cycleway", "mtb", "piste"],
    "paint": {
      "line-color": "@path",
      "line-dasharray": "@path_dasharray_primary",
      "line-width": "@path_width"
    }
  }, {
    "id": "road_path_steps",
    "type": "line",
    "source": "mapbox",
    "source-layer": "road",
    "filter": ["==", "type", "steps"],
    "paint": {
      "line-color": "@path",
      "line-dasharray": [0.3, 0.2],
      "line-width": "@path_steps_width"
    }
  }, {
    "id": "road_major_label",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "road_label",
    "filter": ["all", ["in", "class", "motorway", "main"], ["==", "$type", "LineString"]],
    "layout": {
      "symbol-placement": "line",
      "text-field": "@name",
      "text-font": "@sans_md",
      "text-transform": "uppercase",
      "text-letter-spacing": 0.1,
      "text-max-size": 12
    },
    "paint": {
      "text-color": "@label",
      "text-halo-color": "@label-halo",
      "text-halo-width": 2,
      "text-size": {
        "base": 1.4,
        "stops": [[10, 8], [20, 12]]
      }
    }
  }, {
    "id": "admin_country",
    "type": "line",
    "source": "mapbox",
    "source-layer": "admin",
    "filter": ["all", ["<=", "admin_level", 2], ["==", "maritime", 0], ["==", "$type", "LineString"]],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "#444",
      "line-width": {
        "base": 1.3,
        "stops": [[3, 0.5], [22, 15]]
      }
    }
  }, {
    "id": "country_label",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "country_label",
    "maxzoom": 12,
    "filter": ["==", "$type", "Point"],
    "layout": {
      "text-field": "@name",
      "text-font": "@sans",
      "text-max-size": 22,
      "text-max-width": 10,
      "text-transform": "uppercase"
    },
    "paint": {
      "text-color": "@label",
      "text-halo-color": "@label-halo",
      "text-halo-width": 2,
      "text-size": {
        "stops": [[3, 12], [8, 20]]
      }
    }
  }, {
    "id": "place_label_city",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "place_label",
    "maxzoom": 16,
    "filter": ["all", ["==", "$type", "Point"], ["==", "type", "city"]],
    "layout": {
      "text-field": "@name",
      "text-font": "@sans_md",
      "text-max-width": 10,
      "text-transform": "uppercase"
    },
    "paint": {
      "text-color": "@label",
      "text-halo-color": "@label-halo",
      "text-halo-width": 2,
      "text-size": {
        "stops": [[3, 10], [8, 15]]
      }
    }
  }, {
    "id": "place_label_other",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "place_label",
    "minzoom": 8,
    "filter": ["all", ["==", "$type", "Point"], ["in", "type", "town", "village", "hamlet", "suburb", "neighbourhood"]],
    "layout": {
      "text-field": "@name",
      "text-font": "@sans",
      "text-max-width": 6,
      "text-transform": "uppercase"
    },
    "paint": {
      "text-color": "@label",
      "text-halo-color": "@label-halo",
      "text-halo-width": 1.5,
      "text-size": {
        "stops": [[6, 10], [12, 12]]
      }
    }
  }, {
    "id": "poi_label",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "poi_label",
    "minzoom": 5,
    "filter": ["all", ["==", "scalerank", 1], ["==", "localrank", 1], ["==", "$type", "Point"]],
    "layout": {
      "icon-image": "{maki}-12",
      "icon-max-size": 1,
      "icon-optional": true,
      "text-offset": [0, 0.5],
      "text-field": "@name",
      "text-font": "@sans_it",
      "text-max-size": 11,
      "text-max-width": 8,
      "text-anchor": "top"
    },
    "paint": {
      "icon-size": 1,
      "text-color": "@label",
      "text-halo-width": 1,
      "text-halo-color": "@label-halo",
      "text-size": 11
    }
  }, {
    "id": "poi_label_1-2",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "poi_label",
    "minzoom": 9,
    "filter": ["all", ["in", "scalerank", 1, 2], ["==", "$type", "Point"]],
    "layout": {
      "icon-image": "{maki}-12",
      "text-field": "@name",
      "text-font": "@sans_it",
      "text-max-size": 12,
      "text-max-width": 10,
      "text-offset": [0, 0.6],
      "text-anchor": "top"
    },
    "paint": {
      "text-color": "@label",
      "text-halo-color": "@label-halo",
      "text-size": 10,
      "text-halo-width": 1
    }
  }, {
    "id": "poi_label_3",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "poi_label",
    "minzoom": 16,
    "filter": ["all", ["==", "scalerank", 3], ["==", "$type", "Point"]],
    "layout": {
      "icon-image": "{maki}-12",
      "text-field": "@name",
      "text-font": "@sans_it",
      "text-max-size": 10,
      "text-max-width": 10,
      "text-offset": [0, 0.6],
      "text-anchor": "top"
    },
    "paint": {
      "text-color": "@label",
      "text-halo-color": "@label-halo",
      "text-size": 9,
      "text-halo-width": 1
    }
  }, {
    "id": "poi_label_4",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "poi_label",
    "minzoom": 18,
    "filter": ["all", ["==", "scalerank", 4], ["==", "$type", "Point"]],
    "layout": {
      "icon-image": "{maki}-12",
      "text-field": "@name",
      "text-font": "@sans_it",
      "text-max-size": 9,
      "text-max-width": 10,
      "text-offset": [0, 0.6],
      "text-anchor": "top"
    },
    "paint": {
      "text-color": "@label",
      "text-halo-color": "@label-halo",
      "text-size": 8,
      "text-halo-width": 1
    }
  }]
}
